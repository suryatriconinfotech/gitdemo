pipeline {
    agent any
    tools{
        jdk 'jdk21'
        maven 'maven'
    }
    stages {
        stage('Git Clone'){
            steps{
           git branch: 'master', changelog: false, poll: false, url: 'https://github.com/suryatriconinfotech/JWT-Demo.git'
            }
        }
        stage('Compile'){
            steps{
                bat "mvn clean compile"
            }
        }
        stage('Build'){
            steps{
                bat "mvn clean install"
            }
        }
          stage('Test'){
            steps{
                echo "Testing..."
            }
        }
    }
   
}
